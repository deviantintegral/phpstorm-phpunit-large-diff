{
    "name": "deviantintegral/phpstorm-phpunit-large-diff",
    "description": "Shows bug in large assertion diffs",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Berry",
            "email": "deviantintegral@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
